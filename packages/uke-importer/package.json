		"start": "tsx src/index.ts"
	},
	"devDependencies": {
		"@openbts/drizzle": "workspace:*",
		"@openbts/typescript-configs": "workspace:*",
		"@types/node": "20",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"cheerio": "1.2.0",
		"dotenv": "17.2.3",
		"drizzle-orm": "0.45.1",
		"postgres": "3.4.8",
		"tsx": "4.21.0",
		"xlsx": "0.18.5"
	}
